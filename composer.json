{
  "name": "studiomitte/ttaddress-country-relation",
  "type": "typo3-cms-extension",
  "description": "Add a relation to static_countries in tt_address",
  "keywords": [
    "TYPO3",
    "extension",
    "addresses",
    "country"
  ],
  "homepage": "https://studiomitte.com/",
  "authors": [
    {
      "name": "Georg Ringer",
      "role": "Developer"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "typo3/cms-core": "^11.0 || ^12.4 || ^13.4",
    "php": ">=7.4",
    "friendsoftypo3/tt-address": "^5.3 || ^6 || ^7 || ^8 || ^9",
    "evoweb/extender": "^10.1 || ^11",
    "sjbr/static-info-tables": "^11.5 || ^12.4 || ^13.4"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "ttaddress_country_relation"
    }
  },
  "autoload": {
    "psr-4": {
      "StudioMitte\\TtaddressCountryRelation\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/ttaddress-country-relation": "self.version"
  }
}
